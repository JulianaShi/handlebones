<script src="../../js/tvFinder/television.js"></script>

{{#each item}}
<div class="result-cell left-float">
    <a href="/productPage.html#{{id}}"><img src="{{image}}"/></a>
    <a href="/productPage.html#{{id}}">
        <div class="result-title">{{name}}</div>
    </a>

    <div class="description">{{description}}</div>
    <span class="price">${{price}}<span class="cents"></span></span>
    <span class="stars">{{rating}}</span>
</div>
 {{/each}}
