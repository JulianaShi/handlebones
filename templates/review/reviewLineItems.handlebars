<div id="col-labels" class="row-fluid confirm-order-title">
    <div class="span2">
        Your Order
    </div>
    <div class="span1 offset4">
        Qty
    </div>
    <div class="span2">
        Price
    </div>
</div>
{{#each items}}
<div class="row-fluid review-line-item">
<div class="span2 review-line-item-img">
    <img src="{{image}}" />
</div>
<div class="span4 review-line-item-name">
    {{{name}}}
</div>
<div class="span1 review-line-item-qty">
    {{qty}}
</div>
<div class="span2 review-line-item-price">
    ${{price}}
</div>
</div>
{{/each}}
<div id="empty-div" class="row-fluid">
</div>